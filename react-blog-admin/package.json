    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1",
    "reset-css": "^5.0.1",
    "screenfull": "^6.0.2",
    "vite-plugin-imp": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.15.0",
