    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-less": "^5.0.0",
    "sass": "^1.56.0",
    "typescript": "^4.6.3",
    "typescript-plugin-css-modules": "^3.4.0",
    "vite": "^2.9.9",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "1.4.1",
    "vite-plugin-svgr": "^2.2.2"
