{
  "name": "react18-ts-redux-antd-vite2-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc && vite build",
    "build:beta": "vite build --mode beta",
    "build:release": "vite build --mode release",
    "build:legacy": "vite build --mode legacy ",
    "build:deploy": "vite build --base=/vite-react-ts/",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "stylelint-check": "stylelint-config-prettier-check",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prebuild": "rimraf dist",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@reduxjs/toolkit": "^1.8.5",
    "@types/react-loadable": "^5.5.6",
    "antd": "^4.24.2",
    "axios": "^0.27.2",
    "dayjs": "^1.10.6",
    "i18next": "^21.8.10",
    "js-md5": "^0.7.3",
    "mockjs": "^1.1.0",
    "nanoid": "^4.0.0",
    "jwt-decode": "^2.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.17.3",
    "react-icons": "^4.4.0",
    "react-loadable": "^5.5.0",
		"react-redux": "7.2.6",
    "react-refresh": "0.8.3",
    "react-router-dom": "^6.2.1",
    "react-router-waiter": "^1.1.5",
    "react-scripts": "4.0.0",
