<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cy.single.blog.dao.BlogLabelMapper">



    <sql id="Table_Name">
        blog_label
    </sql>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,surrogate_id, `number`, `name`, remark, deleted, creator_id,modifier_id,create_time,update_time
    </sql>


    <insert id="save">
    </insert>

    <delete id="editById">
        update <include refid="Table_Name" />
        <set>
            <if test="param.number != null and param.number != ''">
                number = #{param.number},
            </if>
            <if test="param.name != null and param.name != ''">
                name = #{param.name},
            </if>
            <if test="param.remark != null and param.remark != ''">
                remark = #{param.remark},
            </if>
            <if test="param.modifierId != null and param.modifierId != ''">
                modifier_id = #{param.modifierId},
            </if>
            <if test="param.updateTime != null and param.updateTime != ''">
                update_time = #{param.update_time},
            </if>
        </set>
        <where>
            surrogate_id = #{param.surrogateId}
        </where>
    </delete>


    <delete id="delete">
    </delete>
</mapper>
